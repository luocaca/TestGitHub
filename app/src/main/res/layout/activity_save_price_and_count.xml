<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_mainView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    tools:context="com.hldj.hmyg.saler.SaveSeedlingActivity">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/y34"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="修改苗木资源"
            android:textColor="@color/main_color"
            android:textSize="@dimen/x18" />

        <ImageView
            android:id="@+id/btn_back"
            android:layout_width="@dimen/x24"
            android:layout_height="@dimen/x24"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x10"
            android:background="@drawable/btn_back" />
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/line" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@color/gray_bg">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:id="@+id/ll_02"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y34"
                android:background="@drawable/feedback_content_edittext_shape"
                android:orientation="horizontal"
                android:padding="1px">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/more_item_selector"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:text="库存"
                        android:textSize="@dimen/x15" />

                    <EditText
                        android:id="@+id/et_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="请输入库存"
                        android:inputType="number"
                        android:paddingLeft="@dimen/x12"
                        android:textSize="@dimen/x15" />
                </LinearLayout>
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/y31"
                android:background="@drawable/feedback_content_edittext_shape"
                android:paddingLeft="@dimen/x9"
                android:paddingRight="@dimen/x9"
                android:paddingTop="@dimen/y10">

                <TextView
                    android:id="@+id/tv_price_"
                    style="@style/tv_seeding_include_bottom_left"
                    android:text=" 价格" />

                <LinearLayout
                    android:id="@+id/ll_center"
                    android:layout_width="@dimen/x180"
                    android:layout_height="@dimen/y14"
                    android:layout_toRightOf="@+id/tv_price_"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/x20">
                    <!--android:paddingLeft="@dimen/x14"-->
                    <!--android:paddingRight="@dimen/x14"-->

                    <EditText
                        android:id="@+id/et_price"
                        style="@style/edit_seeding_imput" />

                    <TextView style="@style/text_Seeding_line" />

                    <EditText
                        android:id="@+id/et_price_max"
                        style="@style/edit_seeding_imput" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/ll_center"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/text_seeding_cm"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="元"
                        android:textSize="@dimen/x14" />

                    <CheckBox
                        android:id="@+id/cb_is_meet"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:button="@drawable/dagou_seletor"
                        android:padding="3dp"
                        android:text="面议"
                        android:textColor="@color/text_color"
                        android:textSize="@dimen/x14" />


                </LinearLayout>


            </RelativeLayout>


