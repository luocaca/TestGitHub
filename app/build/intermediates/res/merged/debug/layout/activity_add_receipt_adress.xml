<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    tools:context="com.hldj.hmyg.buyer.AddReceiptAdressActivity" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/y34"
        android:background="@color/white" >

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="新增收货地址"
            android:textColor="@color/main_color"
            android:textSize="@dimen/x18" />

        <Button
            android:id="@+id/edit_btn"
            android:layout_width="@dimen/x48"
            android:layout_height="@dimen/x24"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x15"
            android:background="@drawable/red_btn_selector"
            android:gravity="center"
            android:text="删除"
            android:textColor="@color/red"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/btn_back"
            android:layout_width="@dimen/x24"
            android:layout_height="@dimen/x24"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x10"
            android:background="@drawable/btn_back" />
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/line" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/gray_bg" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/ll_02"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y34"
                android:background="@drawable/feedback_content_edittext_shape"
                android:orientation="horizontal"
                android:padding="1px" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/btn_my_list"
                    android:orientation="horizontal"
                    android:paddingRight="@dimen/x7" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:drawableLeft="@drawable/seller_redstar"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:text="所在区域"
                        android:textSize="@dimen/x15" />

                    <TextView
                        android:id="@+id/tv_area"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:textColor="@color/gray"
                        android:textSize="@dimen/x15" />

                    <ImageView
                        style="@style/more_triangle"
                        android:background="@drawable/ic_arrow_right" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_03"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y34"
                android:background="@drawable/feedback_content_edittext_shape"
                android:orientation="horizontal"
                android:padding="1px"
                android:visibility="gone" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/btn_my_list"
                    android:orientation="horizontal"
                    android:paddingRight="@dimen/x7" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:text="所在街道"
                        android:textSize="@dimen/x15" />

                    <TextView
                        android:id="@+id/tv_street"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:textColor="@color/gray"
                        android:textSize="@dimen/x15" />

                    <ImageView
                        style="@style/more_triangle"
                        android:background="@drawable/ic_arrow_right" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_04"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y34"
                android:background="@drawable/feedback_content_edittext_shape"
                android:orientation="horizontal"
                android:padding="1px"
                android:visibility="gone" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/btn_my_list"
                    android:orientation="horizontal"
                    android:paddingRight="@dimen/x7" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:text="地图标注"
                        android:textSize="@dimen/x15" />

                    <TextView
                        android:id="@+id/tv_map"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:textColor="@color/gray"
                        android:textSize="@dimen/x15" />

                    <ImageView
                        style="@style/more_triangle"
                        android:background="@drawable/ic_arrow_right" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_05"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y34"
                android:background="@drawable/feedback_content_edittext_shape"
                android:orientation="horizontal"
                android:padding="1px"
                android:visibility="gone" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/more_item_selector"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:text="联系地址："
                        android:textSize="@dimen/x15" />

                    <EditText
                        android:id="@+id/et_adress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:textSize="@dimen/x15" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_06"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y34"
                android:background="@drawable/feedback_content_edittext_shape"
                android:orientation="horizontal"
                android:padding="1px" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/more_item_selector"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:text="收货人："
                        android:textSize="@dimen/x15" />

                    <EditText
                        android:id="@+id/et_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:textSize="@dimen/x15" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_07"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y34"
                android:background="@drawable/feedback_content_edittext_shape"
                android:orientation="horizontal"
                android:padding="1px" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/more_item_selector"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:drawableLeft="@drawable/seller_redstar"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/x12"
                        android:text="收货电话："
                        android:textSize="@dimen/x15" />

                    <EditText
                        android:id="@+id/et_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:inputType="number"
                        android:maxLength="11"
                        android:paddingLeft="@dimen/x12"
                        android:textSize="@dimen/x15" />
                </LinearLayout>
            </LinearLayout>

            <com.amap.api.maps2d.MapView
                android:id="@+id/map"
                android:layout_width="fill_parent"
                android:layout_height="1px"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50.0dip"
                android:orientation="horizontal" >

                <CheckBox
                    android:id="@+id/remmber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10.0dip"
                    android:button="@drawable/agreement_checkbox_bg"
                    android:checked="false"
                    android:text="设为默认地址"
                    android:textColor="#5A5A5A"
                    android:textSize="15.0sp" />
            </LinearLayout>

            <com.neopixl.pixlui.components.textview.TextView
                android:id="@+id/save"
                style="@style/NoActionBarTheme.TextHuaWenXiHei"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x37"
                android:layout_marginLeft="@dimen/x10"
                android:layout_marginRight="@dimen/x10"
                android:layout_marginTop="@dimen/x10"
                android:background="@drawable/register_btn_selector"
                android:enabled="true"
                android:gravity="center"
                android:text="保存"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/white"
                android:textSize="@dimen/y11" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>